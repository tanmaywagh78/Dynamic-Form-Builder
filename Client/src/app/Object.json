{
  "firstName": {
    "type": "textbox",
    "name": "firstName",
    "label": "First Name",
    "value": "",
    "required": "true"
  },
  "lastName": {
    "type": "textbox",
    "name": "lastName",
    "label": "Last Name",
    "value": "",
    "required": "true"
  },
  "email": {
    "type": "textbox",
    "name": "email",
    "label": "Email",
    "value": "",
    "required": "true"
  },
  "picture": {
    "type": "file",
    "name": "picture",
    "label": "Picture",
    "required": "true",
    "onUpload": "this.onUpload.bind(this)"
  },
  "country": {
    "type": "dropdown",
    "name": "country",
    "label": "Country",
    "value": "in",
    "required": "true",
    "options": [
      {
        "key": "in",
        "label": "India"
      },
      {
        "key": "us",
        "label": "USA"
      }
    ]
  },
  "gender": {
    "type": "radio",
    "name": "gender",
    "label": "Gender",
    "value": "m",
    "required": "true",
    "options": [
      {
        "key": "m",
        "label": "Male"
      },
      {
        "key": "f",
        "label": "Female"
      }
    ]
  },
  "hobbies": {
    "type": "checkbox",
    "name": "hobbies",
    "label": "Hobbies",
    "required": "true",
    "options": [
      {
        "key": "f",
        "label": "Fishing"
      },
      {
        "key": "c",
        "label": "Cooking"
      }
    ]
  }
